{
  "expo": {
    "name": "lecafe-app",
    "slug": "lecafe-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6295"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.gerzon.lecafe-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF6295"
      },
      "package": "com.gerzon.lecafeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MavenPro-Bold.ttf",
            "./assets/fonts/MavenPro-Regular.ttf",
            "assets/fonts/MavenPro-SemiBold.ttf",
            "./assets/fonts/Quicksand-Bold.ttf",
            "./assets/fonts/Quicksand-Regular.ttf",
            "./assets/fonts/Quicksand-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
